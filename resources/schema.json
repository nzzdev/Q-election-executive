{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "annotations": {
      "type": "string"
    },
    "intermediate": {
      "type": "boolean"
    },
    "majority": {
      "type": "number"
    },
    "candidates": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "party": {
            "type": "string"
          },
          "previous": {
            "type": "boolean"
          },
          "votes": {
            "type": "number"
          },
          "status": {
            "enum": [
              "elected",
              "not elected",
              "undefined"
            ]
          },
          "colour": {
            "type": "string"
          },
          "image": {
            "type": "string"
          }
        }
      }
    },
    "notes": {
      "type": "string"
    },
    "sources": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string"
          },
          "href": {
            "type": "string"
          },
          "validHref": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": [
    "title"
  ]
}
